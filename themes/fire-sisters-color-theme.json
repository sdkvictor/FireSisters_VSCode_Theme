  "semanticTokenColors": {
    "variable.readonly": "#AEB8D6",
    "variable.defaultLibrary": "#AEB8D6",
    "property": "#AEB8D6",
    "function": "#7BE8C6",
    "method": "#7BE8C6",
    "class": "#F5D061",
    "interface": "#F5D061",
    "type": "#F5D061",
    "enum": "#F5D061",
    "parameter": "#A7B8FF",
    "namespace": "#4F5D84",
    "enumMember": "#63D6B0"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#4F5D84",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#F5D061"
      }
    },
    {
      "scope": [
        "variable", 
        "variable.other.constant.js",
        "variable.other.block.js",
        "variable.other.readwrite.js",
        "variable.other.js"
      ],
      "settings": {
        "foreground": "#AEB8D6"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#7BE8C6"
      }
    },
    {
      "scope": ["number", "constant"],
      "settings": {
        "foreground": "#F5D061"
      }
    },
    {
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#F5D061"
      }
    },
    {
      "scope": [
        "entity.name.function.js",
        "support.function"
      ],
      "settings": {
        "foreground": "#7BE8C6"
      }
    },
    {
      "scope": [
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#F5D061"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#63D6B0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#7BE8C6"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#AEB8D6"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6E78A8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.type.python",
        "entity.name.type.class.python"
      ],
      "settings": {
        "foreground": "#F5D061"
      }
    },
    {
      "scope": [
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#7BE8C6"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.property.object",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#9FAAC9"
      }
    },
    {
      "scope": [
        "meta.method-call",
        "support.function.method"
      ],
      "settings": {
        "foreground": "#7BE8C6"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#D9C26A"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#D9B56A"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#E0C35A"
      }
    },
    {
      "name": "Brackets & punctuation",
      "scope": [
        "punctuation.section.block",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "punctuation.definition.block",
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Variables & identifiers",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#B8BACF"
      }
    },
    {
      "name": "Function parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#A7B8FF"
      }
    },
  ]
  ,
  "semanticHighlighting": true,
  
  "editorCursor.width": 2,
  "debugConsole.background": "#0A0B10",
  "debugConsole.foreground": "#E6E6EB",
  "editorBracketHighlight.foreground1": "#9FAAC9"

}